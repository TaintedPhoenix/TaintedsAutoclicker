<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="130.0" prefWidth="200.0" stylesheets="@main.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.TaintedPhoenix.Main">
   <children>
      <Label layoutX="10.0" layoutY="10.0" text="Selected Keys:" />
      <Label fx:id="KS_label_info" layoutX="10.0" layoutY="30.0" prefHeight="25.0" prefWidth="180.0" text="Keys shown here" textAlignment="CENTER" />
      <Button fx:id="KS_button_start" layoutX="10.0" layoutY="60.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="85.0" styleClass="controlButton" text="Start" />
      <Button fx:id="KS_button_stop" layoutX="105.0" layoutY="60.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="85.0" styleClass="controlButton" text="Stop" />
      <Button fx:id="KS_button_ok" layoutX="10.0" layoutY="95.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="180.0" styleClass="controlButton" text="Done" />
   </children>
</AnchorPane>
